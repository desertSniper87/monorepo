[Sokoban YASC Skin Script]
SkinType=Common Skins (2019)
FileType=Image
FileFilter=
Columns=4..
Rows=1..
UseBackground=No

====================
Common Skins (2019)
====================

-- Add extra script lines from the image, if any. (They are not visually readable.)
GetImageText {Tile 4, 4} "[" {Filename.Shortname} "]"

if {Rows} = 6
   perform Common_Skins
else
if {Rows} = 8
   perform Common_Skins
else
if {Rows} = 10
   perform Common_Skins
else
if {Rows} = 12
   perform Common_Skins
else
if {Rows} = 1
   perform Simple_1_Row_Skin
else
if {Rows} = 2
   perform Common_Skins_2_or_4_Row_Skin
else
if {Rows} = 4
   perform Common_Skins_2_or_4_Row_Skin
endif
endif
endif
endif
endif
endif
endif

if {Self.{Filename.Shortname}.Background color tolerance (%)} = {}
  (do nothing)
else
perform Background_Color_Tolerance
endif

if {Self.{Filename.Shortname}.Directional player}
perform Directional_Player
endif

-- Handle skin-specific settings, if any.
perform {Filename.Shortname}

end -- script

